{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@store/*": ["src/store/*"],
      "@pages/*": ["src/pages/*"],
      // Add more aliases if needed
    },
    "module": "ES2020",
    "moduleResolution": "bundler",
    "target": "es6",
    "jsx": "react",
  },
  "include": ["./src/**/*"]
  
}
